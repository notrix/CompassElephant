{
    "name": "notrix/compass-elephant",
    "description": "A library to manage a compass project with PHP",
    "keywords": ["compass", "sass", "css"],
    "homepage": "http://matteosister.github.com/CompassElephant",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/process": "~2.1|~3.0",
        "symfony/finder": "~2.1|~3.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "CompassElephant\\": "src/CompassElephant"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3|~5.0"
    }
}
